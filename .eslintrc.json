{
    "env": {
        "es2020": true
    },
    "extends": ["plugin:@typescript-eslint/recommended"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "plugins": ["@typescript-eslint"],
    "rules": {
        "indent": "off",
        "linebreak-style": "off",
        "require-jsdoc": "off",
        "object-curly-spacing": "off",
        "no-new-require": "off"
    }
}
